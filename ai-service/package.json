{
  "name": "sales-ai-service",
  "version": "3.6.3",
  "description": "AI Agent Microservice for Sales Tracker App",
  "main": "server.js",
  "scripts": {
    "start": "node react-agent-enhanced.js",
    "start:email": "node react-agent-enhanced-with-email.js",
    "start:docs": "node react-agent-enhanced-with-docs.js",
    "start:legacy": "node server.js",
    "dev": "nodemon react-agent-enhanced.js",
    "dev:email": "nodemon react-agent-enhanced-with-email.js",
    "dev:docs": "nodemon react-agent-enhanced-with-docs.js",
    "dev:legacy": "nodemon server.js",
    "setup:email": "node setup-email-service.js",
    "test": "jest --testTimeout=10000",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/react-agent-enhanced.test.js",
    "test:integration": "jest tests/integration.test.js --testTimeout=20000",
    "test:all": "npm run test:unit && npm run test:integration",
    "docker:build": "docker build -f Dockerfile.enhanced -t ai-agent-enhanced .",
    "docker:run": "docker run -p 3000:3000 --env-file .env ai-agent-enhanced",
    "docker:compose": "docker-compose -f docker-compose.enhanced.yml up",
    "queue:dashboard": "node -e \"console.log('Queue dashboard available at http://localhost:3000/admin/queues')\"",
    "email:dashboard": "node -e \"console.log('Email dashboard available at http://localhost:3000/admin/email')\"",
    "docs:validate": "npx @apidevtools/swagger-cli validate api-docs/openapi.yaml",
    "docs:serve": "npm run start:docs && open http://localhost:3000/api-docs"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^11.0.1",
    "@azure/msal-node": "^2.6.0",
    "@bull-board/api": "^6.10.0",
    "@bull-board/express": "^6.10.0",
    "@google-cloud/aiplatform": "^3.10.0",
    "@google-cloud/local-auth": "^3.0.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@sendgrid/mail": "^8.1.5",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "chart.js": "^4.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "d3-node": "^3.0.0",
    "dotenv": "^16.5.0",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "gtts": "^0.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.2.0",
    "ioredis": "^5.6.1",
    "jimp": "^1.6.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "juice": "^11.0.1",
    "luxon": "^3.4.4",
    "markdown-pdf": "^11.0.0",
    "mjml": "^4.15.3",
    "node-cache": "^5.1.2",
    "node-html-parser": "^7.0.1",
    "nodemailer": "^7.0.3",
    "open": "^10.1.2",
    "pdfkit": "^0.15.1",
    "pg": "^8.16.0",
    "say": "^0.16.0",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "eventsource": "^2.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
